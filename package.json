{
  "name": "js-convert-case",
  "version": "4.0.0",
  "description": "JavaScript Convert String and Keys of Object between cases (camelCase, snake_case, PascalCase, dot.case, path/case, text case, Sentence case, Header Case, UPPERCASE, lowercase). Use for both Node.JS and Browser",
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "dist:dep": "yarn dist:dep:1 && yarn dist:dep:2",
    "dist:dep:1": "yarn js-camelcase && yarn js-dotcase && yarn js-headercase && yarn js-pascalcase && yarn js-pathcase&& yarn js-sentencecase && yarn js-snakecase && yarn js-textcase",
    "dist:dep:2": "yarn camelcase-keys-object && yarn lowercase-keys-object && yarn pascalcase-keys-object && yarn snakecase-keys-object && yarn uppercase-keys-object",
    "js-camelcase": "rollup -c packages/js-camelcase/rollup.config.js",
    "js-dotcase": "rollup -c packages/js-dotcase/rollup.config.js",
    "js-headercase": "rollup -c packages/js-headercase/rollup.config.js",
    "js-pascalcase": "rollup -c packages/js-pascalcase/rollup.config.js",
    "js-pathcase": "rollup -c packages/js-pathcase/rollup.config.js",
    "js-sentencecase": "rollup -c packages/js-sentencecase/rollup.config.js",
    "js-snakecase": "rollup -c packages/js-snakecase/rollup.config.js",
    "js-textcase": "rollup -c packages/js-textcase/rollup.config.js",
    "camelcase-keys-object": "rollup -c packages/camelcase-keys-object/rollup.config.js",
    "lowercase-keys-object": "rollup -c packages/lowercase-keys-object/rollup.config.js",
    "pascalcase-keys-object": "rollup -c packages/pascalcase-keys-object/rollup.config.js",
    "snakecase-keys-object": "rollup -c packages/snakecase-keys-object/rollup.config.js",
    "uppercase-keys-object": "rollup -c packages/uppercase-keys-object/rollup.config.js"
  },
  "dependencies": {
    "camelcase-keys-object": "file:./packages/camelcase-keys-object",
    "js-camelcase": "file:./packages/js-camelcase",
    "js-dotcase": "file:./packages/js-dotcase",
    "js-headercase": "file:./packages/js-headercase",
    "js-pascalcase": "file:./packages/js-pascalcase",
    "js-pathcase": "file:./packages/js-pathcase",
    "js-sentencecase": "file:./packages/js-sentencecase",
    "js-snakecase": "file:./packages/js-snakecase",
    "js-textcase": "file:./packages/js-textcase",
    "lowercase-keys-object": "file:./packages/lowercase-keys-object",
    "pascalcase-keys-object": "file:./packages/pascalcase-keys-object",
    "snakecase-keys-object": "file:./packages/snakecase-keys-object",
    "uppercase-keys-object": "file:./packages/uppercase-keys-object"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "rollup": "^2.10.8",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "files": [
    "dist/**/*",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huynhsamha/js-convert-case.git"
  },
  "author": {
    "name": "huynhsamha",
    "email": "huynhsamha@gmail.com",
    "url": "https://github.com/huynhsamha"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huynhsamha/js-convert-case/issues"
  },
  "homepage": "https://github.com/huynhsamha/js-convert-case",
  "contributors": [
    "huynhsamha"
  ],
  "icon": "./docs/favicon.ico",
  "keywords": [
    "js-convert-case",
    "js-camelcase",
    "js-dotcase",
    "js-headercase",
    "js-pascalcase",
    "js-pathcase",
    "js-sentencecase",
    "js-snakecase",
    "js-textcase",
    "lowercase-keys-object",
    "uppercase-keys-object",
    "snakecase-keys-object",
    "camelcase-keys-object",
    "pascalcase-keys-object"
  ]
}
